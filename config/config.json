{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "modern-calendar-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/modernCalendar/ModernCalendarWebPart.js",
          "manifest": "./src/webparts/modernCalendar/ModernCalendarWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "https://code.jquery.com/jquery-1.12.4.min.js",
    "moment": "https://momentjs.com/downloads/moment.min.js"
  },
  "localizedResources": {
    "ModernCalendarWebPartStrings": "lib/webparts/modernCalendar/loc/{locale}.js"
  }
}
